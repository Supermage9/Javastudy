package com.constex;

public class BookEx {
	
	String title;
	String author;
	
	public void show() {
		
	}
	
	public BookEx() {
		this("","");
		System.out.println("생성자 호출함");
	}
	
	public BookEx(String title) {
		this(title,"작자미상");
		
	}
	
	public BookEx(String title, String author) {
		this.title = title;
		this.author = author;
	}
	
	
	

	public static void main(String[] args) {
		
		BookEx be = new BookEx("",null);
		
		
		
		
		
		
		
	}

}
