package com.inherex;

class Super{ // 부모클래스
	
	int a = 5; //부모클래스의 멤버변수
}
class Sub extends Super{
	
	int a = 10;
	
	public void test() {
		System.out.println(this.a);
		System.out.println(super.a);
	}
}


public class SuperEx {
	public static void main(String[] args) {
		Sub a = new Sub();
		
		a.test();
		
	}

}
